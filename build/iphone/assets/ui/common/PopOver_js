function PopOver(regged)
{
	self=Ti.UI.iPad.createPopover({
		height:400,
		width:300
	});
	
	if(regged===false
	){
		self.title = "Rediger antal";
		self.arrowDirection = Ti.UI.iPad.POPOVER_ARROW_DIRECTION_LEFT;
		
	}else{
		self.title = "Rediger antal";
		self.arrowDirection = Ti.UI.iPad.POPOVER_ARROW_DIRECTION_RIGHT;
		
		
	};


self.tester = function(){
	console.log("hej");
};
	return self;
	
}
module.exports = PopOver;